{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es2020",
            "ESNext.AsyncIterable",
            "dom"
        ],
        "noImplicitAny": true,
        "outDir": "typeScript",
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": true,
}